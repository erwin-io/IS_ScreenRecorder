{
    "manifest_version" : 2,
    "name" : "IS Screen Recorder",
    "version" : "1.0",
    "description" : "Screen Recorder",
    "icons" : {
        "128" : "img/icon.png",
        "48" : "img/icon.png",
        "16" : "img/icon.png"
    },
    "browser_action" : {
        "default_icon" : "img/icon.png",
        "default_popup" : "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}